{
  "name": "vue-styled-components",
  "version": "1.2.1",
  "description": "Visual primitives for the component age. A simple port of styled-components ðŸ’… for Vue",
  "main": "lib/index.js",
  "module": "dist/vue-styled-components.es.js",
  "jsnext:main": "dist/vue-styled-components.es.js",
  "unpkg": "dist/vue-styled-components.min.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:dist",
    "prebuild:lib": "rimraf lib/*",
    "build:lib": "babel --out-dir lib --ignore \"*.test.js\" src",
    "prebuild:dist": "rimraf dist/*",
    "build:dist": "rollup -c && rollup -c --environment ESBUNDLE && rollup -c --environment PRODUCTION",
    "build:watch": "npm run build:lib -- --watch",
    "test": "mocha \"./src/**/*.test.js\" --require babel-core/register --timeout 5000",
    "test:size": "bundlesize",
    "lint": "eslint src",
    "prepublish": "npm run build",
    "lint-staged": "lint-staged",
    "dev": "node example/devServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/styled-components/vue-styled-components.git"
  },
  "files": [
    "CONTRIBUTING.md",
    "CODE_OF_CONDUCT.md",
    "dist",
    "lib",
    "src"
  ],
  "keywords": [
    "vue",
    "css",
    "css-in-js",
    "styled-components"
  ],
  "author": "Lorenzo Girardi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/styled-components/vue-styled-components/issues"
  },
  "dependencies": {
    "fbjs": "^0.8.9",
    "lodash.isplainobject": "^4.0.6",
    "stylis": "3.x"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.24.1",
    "bundlesize": "0.15.3",
    "chokidar": "^1.6.1",
    "danger": "^0.16.0",
    "eslint": "^3.12.2",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-vue": "^2.0.1",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "jsdom": "^9.9.1",
    "lint-staged": "^3.2.4",
    "lodash": "^4.15.0",
    "mocha": "^3.2.0",
    "mocha-jsdom": "^1.1.0",
    "node-watch": "^0.4.1",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1",
    "rollup": "0.50.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-builtins": "^2.1.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "2.0.1",
    "rollup-plugin-visualizer": "0.3.1",
    "rollup-plugin-vue2": "^0.8.0",
    "vue": "^2.2.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "pre-commit": "lint-staged",
  "bundlesize": [
    {
      "path": "./dist/vue-styled-components.min.js",
      "threshold": "14.5kB"
    }
  ]
}
